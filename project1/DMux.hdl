/**
 * Demultiplexor:
 * {a, b} = {in, 0} if sel == 0
 *          {0, in} if sel == 1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Nand(a=in, b=sel, out=temp1);
    Nand(a=in, b=temp1, out=temp2);
    Not(in=temp1, out=b);
    Not(in=temp2, out=a);
}
